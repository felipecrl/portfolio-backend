generator client {
    provider   = "prisma-client-js"
    engineType = "binary"
}

datasource db {
    provider = "postgresql"
}

model User {
    id           String   @id @default(cuid())
    username     String   @unique
    passwordHash String
    role         Role     @default(ADMIN)
    createdAt    DateTime @default(now())
    updatedAt    DateTime @updatedAt
}

enum Role {
    ADMIN
}
